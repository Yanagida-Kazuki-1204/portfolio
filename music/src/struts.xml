<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<constant name="struts.ui.theme" value="simple" />
	<!-- 開発者モード(デバッグしたい時にはコメント外してください。ユーザーのエラーページが無効にできます) -->
	<!-- <constant name="struts.devMode" value="true" /> -->
	<constant name="struts.devMode" value="true" />
	<package name="com.internousdev.music.action" extends="struts-default">

  <action name="AddCartAction" class="com.internousdev.music.action.AddCartAction" method="execute">
		<result name="success">cart.jsp</result>
		<result name="error">home.jsp</result>
		<result name="input">cart.jsp</result>
	</action>

	<action name="CartAction" class="com.internousdev.music.action.CartAction" method="execute">
		<result name="success">cart.jsp</result>
		<result name="error">home.jsp</result>
	</action>

	<action name="CreateDestinationAction" class="com.internousdev.music.action.CreateDestinationAction" method="execute">
		<result name="success">createDestination.jsp</result>
		<result name="error">home.jsp</result>
	</action>

	<action name="CreateDestinationCompleteAction" class="com.internousdev.music.action.CreateDestinationCompleteAction" method="execute">
		<result name="success">createDestinationComplete.jsp</result>
		<result name="error">createDestination.jsp</result>
	</action>

	<action name="CreateDestinationConfirmAction" class="com.internousdev.music.action.CreateDestinationConfirmAction" method="execute">
		<result name="success">createDestinationConfirm.jsp</result>
		<result name="error">createDestination.jsp</result>
	</action>

	<action name="CreateUserAction" class="com.internousdev.music.action.CreateUserAction" method="execute">
		<result name="success">createUser.jsp</result>
		<result name="error">home.jsp</result>
	</action>

    <action name="CreateUserCompleteAction" class="com.internousdev.music.action.CreateUserCompleteAction" method="execute">
		<result name="success">createUserComplete.jsp</result>
		<result name="error">createUser.jsp</result>
	</action>

    <action name="CreateUserConfirmAction" class="com.internousdev.music.action.CreateUserConfirmAction" method="execute">
		<result name="success">createUserConfirm.jsp</result>
		<result name="error">createUser.jsp</result>
	</action>

  <action name="DeleteCartAction" class="com.internousdev.music.action.DeleteCartAction" method="execute">
		<result name="success">cart.jsp</result>
		<result name="error">cart.jsp</result>
	</action>

  <action name="DeletePurchaseHistoryAction" class="com.internousdev.music.action.DeletePurchaseHistoryAction" method="execute">
		<result name="success">createUser.jsp</result>
		<result name="error">home.jsp</result>
	</action>

  <action name="GoLoginAction" class="com.internousdev.music.action.GoLoginAction" method="execute">
		<result name="success">login.jsp</result>
		<result name="error">home.jsp</result>
	</action>

   <action name="HomeAction" class="com.internousdev.music.action.HomeAction" method="execute">
		<result name="success">home.jsp</result>
	</action>

    <action name="LoginAction" class="com.internousdev.music.action.LoginAction" method="execute">
		<result name="success">home.jsp</result>
		<result name="settlement">settlementConfirm.jsp</result>
		<result name="error">login.jsp</result>
	</action>

	<action name="LogoutAction" class="com.internousdev.music.action.LogoutAction" method="execute">
		<result name="success">index.jsp</result>
		<result name="error">index.jsp</result>
	</action>

	<action name="MyPageAction" class="com.internousdev.music.action.MyPageAction" method="execute">
		<result name="success">mypage.jsp</result>
		<result name="error">home.jsp</result>
	</action>

   <action name="ProductDetailsAction" class="com.internousdev.music.action.ProductDetailsAction" method="execute">
		<result name="success">productDetails.jsp</result>
		<result name="error">productList.jsp</result>
	</action>

   <action name="ProductListAction" class="com.internousdev.music.action.ProductListAction" method="execute">
		<result name="success">productList.jsp</result>
		<result name="error">home.jsp</result>
	</action>

	<action name="PurchaseHistoryAction" class="com.internousdev.music.action.PurchaseHistoryAction" method="execute">
		<result name="success">purchaseHistory.jsp</result>
	</action>

	<action name="ResetPasswordAction" class="com.internousdev.music.action.ResetPasswordAction" method="execute">
		<result name="success">resetPassword.jsp</result>
		<result name="error">login.jsp</result>
	</action>

	<action name="ResetPasswordCompleteAction" class="com.internousdev.music.action.ResetPasswordCompleteAction" method="execute">
		<result name="success">resetPasswordComplete.jsp</result>
		<result name="error">resetPassword.jsp</result>
	</action>

	<action name="ResetPasswordConfirmAction" class="com.internousdev.music.action.ResetPasswordConfirmAction" method="execute">
		<result name="success">resetPasswordConfirm.jsp</result>
		<result name="error">resetPassword.jsp</result>
	</action>

   <action name="SearchItemAction" class="com.internousdev.music.action.SearchItemAction" method="execute">
		<result name="success">productList.jsp</result>
		<result name="error">productList.jsp</result>
	</action>

   <action name="SettlementCompleteAction" class="com.internousdev.music.action.SettlementCompleteAction" method="execute">
		<result name="success">settlementComplete.jsp</result>
		<result name="error">settlementConfirm.jsp</result>
	</action>

   <action name="SettlementConfirmAction" class="com.internousdev.music.action.SettlementConfirmAction" method="execute">
		<result name="success">settlementConfirm.jsp</result>
		<result name="error">login.jsp</result>
	</action>

	<action name="ReviewAction" class="com.internousdev.music.action.ReviewAction" method="execute">
		<result name="success">review.jsp</result>
	</action>

	<action name="ReviewConfirmAction" class="com.internousdev.music.action.ReviewConfirmAction" method="execute">
		<result name="success">reviewConfirm.jsp</result>
		<result name="error">review.jsp</result>
	</action>

	<action name="ReviewCompleteAction" class="com.internousdev.music.action.ReviewCompleteAction" method="execute">
		<result name="success">reviewComplete.jsp</result>
		<result name="error">reviewConfirm.jsp</result>
	</action>


	</package>
</struts>